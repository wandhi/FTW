x;object-fit:content\"/><b-form-input id=\"input-small\" size=\"sm\" placeholder=\"Cari kode driver atau nama driver\" style=\"border:0 !important;font-size:12px\"></b-form-input>\n        </b-col>\n      </b-row>\n      <b-row>\n        \n      </b-row>\n        <b-row style=\"margin-top:55px\">\n          <b-col>\n           \n                  <b-card no-body style=\"border:none !important\">\n       \n        <el-table class=\"table-responsive table\"\n                  header-row-class-name=\"thead-light\"\n                  :data=\"projects\">\n            <el-table-column label=\"Code\"\n                             min-width=\"110px\"\n                             prop=\"code\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;\" class=\"font-weight-400 name mb-0 text-sm\">{{row.code}}</span>\n                        </b-media-body>\n                    </b-media>\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Driver\"\n                             min-width=\"110px\"\n                             prop=\"driver\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;\" class=\"font-weight-400 name mb-0 text-sm\">{{row.driver}}</span>\n                        </b-media-body>\n                    </b-media>\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Jam Siap\"\n                             min-width=\"110px\"\n                             prop=\"jamSiap\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;\" class=\"font-weight-400 name mb-0 text-sm\">{{row.jamSiap}}</span>\n                        </b-media-body>\n                    </b-media>\n                </template>\n            </el-table-column>\n            \n            <el-table-column label=\"Jam Masuk\"\n                             min-width=\"110px\"\n                             prop=\"jamMasuk\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;color:#F8B551\" class=\"font-weight-400 name mb-0 text-sm\">{{row.jamMasuk}}</span>\n                        </b-media-body>\n                    </b-media>\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Jam Pulang\"\n                             min-width=\"110px\"\n                             prop=\"jamMasuk\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;color:#59D185\" class=\"font-weight-400 name mb-0 text-sm\">{{row.jamMasuk}}</span>\n                        </b-media-body>\n                    </b-media>\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Siklus Tgl\"\n                             min-width=\"110px\"\n                             prop=\"siklusTanggal\">\n                <template v-slot=\"{row}\">\n                    <b-media no-body class=\"align-items-center\">\n                        \n                        <b-media-body>\n                            <span style=\"font-size:12px;\" class=\"font-weight-400 name mb-0 text-sm\">{{row.siklusTanggal}}</span>\n  